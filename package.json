{
  "name": "playwright-applitools-visual-testing",
  "version": "1.0.0",
  "description": "Complete boilerplate for visual testing using Playwright and Applitools Eyes SDK",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:debug": "jest --verbose",
    "test:basic": "jest tests/basic-visual.test.ts",
    "test:advanced": "jest tests/advanced-visual.test.ts",
    "test:grid": "jest tests/visual-grid.test.ts",
    "test:example": "jest tests/example.test.ts",
    "test:automated": "jest tests/automated-screenshot-comparison.test.ts",
    "test:with-key": "npx tsx scripts/run-with-api-key.ts",
    "test:basic-with-key": "npx tsx scripts/run-with-api-key.ts --apiKey=$npm_config_apikey npm run test:basic",
    "test:advanced-with-key": "npx tsx scripts/run-with-api-key.ts --apiKey=$npm_config_apikey npm run test:advanced",
    "test:grid-with-key": "npx tsx scripts/run-with-api-key.ts --apiKey=$npm_config_apikey npm run test:grid",
    "test:all-with-key": "npx tsx scripts/run-with-api-key.ts --apiKey=$npm_config_apikey npm test",
    "setup:script": "npx tsx scripts/setup.ts",
    "install:playwright": "npx playwright install",
    "setup": "npm install && npm run install:playwright && npm run build"
  },
  "keywords": [
    "playwright",
    "applitools",
    "visual-testing",
    "typescript",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/pngjs": "^6.0.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.3.0",
    "jest": "^29.6.4",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@applitools/eyes-playwright": "^1.20.2",
    "@playwright/test": "^1.37.1",
    "expect-playwright": "^0.8.0",
    "playwright": "^1.37.1"
  }
}
